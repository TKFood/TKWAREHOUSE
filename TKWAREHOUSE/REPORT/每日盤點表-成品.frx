        <Column Name="業務" DataType="System.String"/>
        <Column Name="效期內的訂單需求量" DataType="System.Int32"/>
        <Column Name="效期內的訂單差異量" DataType="System.Int32"/>
        <Column Name="生產日期" DataType="System.String"/>
        <Column Name="狀態" DataType="System.String"/>
      </TableDataSource>
    </MsSqlDataConnection>
